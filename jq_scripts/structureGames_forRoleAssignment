[
  .awesome[]? |
    (try if ((. != []) and ([.[0] | .participants | .[] | .timeline.lane] | contains(["NONE"]))) then empty else
    { "gameId": (try .[0].gameId catch "0000"),
    "participants": (try .[0].participants catch "ZERO")
    | (try [[.[] | select(.stats.win==true)], [.[] | select(.stats.win==false)]] catch "FIRST")
    | (try if ([.[0][].timeline.lane] | sort) == ["BOTTOM", "BOTTOM", "JUNGLE", "MIDDLE", "TOP"] and
      ([.[1][].timeline.lane] | sort) == ["BOTTOM", "BOTTOM", "JUNGLE", "MIDDLE", "TOP"] then
    ( [
        (.[0][] | select(.timeline.lane=="TOP")),
        (.[0][] | select(.timeline.lane=="JUNGLE")),
        (.[0][] | select(.timeline.lane=="MIDDLE")),
        ([.[0][] | select(.timeline.lane=="BOTTOM")] | max_by(.stats.totalMinionsKilled)),
        ([.[0][] | select(.timeline.lane=="BOTTOM")] | min_by(.stats.totalMinionsKilled)),
        (.[1][] | select(.timeline.lane=="TOP")),
        (.[1][] | select(.timeline.lane=="JUNGLE")),
        (.[1][] | select(.timeline.lane=="MIDDLE")),
        ([.[1][] | select(.timeline.lane=="BOTTOM")] | max_by(.stats.totalMinionsKilled)),
        ([.[1][] | select(.timeline.lane=="BOTTOM")] | min_by(.stats.totalMinionsKilled))
      ]) else empty end catch "SECOND")
    | (try  [.[] | {"participantId":.participantId, "championId":.championId, "spell1Id":.spell1Id, "spell2Id": .spell2Id,
                    "kills": .stats.kills, "deaths": .stats.deaths, "assists": .stats.assists, "earned": .stats.goldEarned,
                    "minionsKilled": .stats.totalMinionsKilled, "neutralMinionsKilled": .stats.neutralMinionsKilled,
                    "wardsPlaced": .stats.wardsPlaced,"level": .stats.champLevel, "lane": .timeline.lane, "role": .timeline.role}] catch "THIRD")
    }
    end
    catch "ERROR")
]
